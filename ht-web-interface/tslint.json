{
  "extends": [
    "tslint:recommended",
    "tslint-no-circular-imports",
    "tslint-react"
  ],
  "rules": {
    "no-var-requires": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "jsx-no-multiline-js": false,
    "max-line-length": [
      true,
      140
    ]
  }
}